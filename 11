#include <stdio.h>
int main()
{
  float a,b,d,c,z,q,w,e,r,z1,q1,w1,e1,r1;
  a=3;b=2;c=4;d=5;
  z = !( a>b ); /* ! логичское не*/
  z1 = (a<b);
  q = ! ( a<2 || a>5); // ||логисемкое или
  q1 = (a>2 || a<5);
  w = ! ( 2*a == b+4 ); // равно
  w1 = (2*a != b+4); // не равно
  e = ! ( a<1 || b<2 && c<3 );
  e1 = (a>1 || b>2 && c >3);
  r = ! ( a<b && c<d );
  r1 = (a>b && c>d);
  printf("значение %f %f \n %f %f \n %f %f \n %f %f \n", z,z1,q,q1,w,w1,e,e1,r,r1);
}
